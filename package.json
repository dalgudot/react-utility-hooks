{
  "name": "@dalgu/react-utility-hooks",
  "description": "Frequently used React Custom Hooks",
  "author": "KyungHwan Kim",
  "version": "0.1.3",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dalgudot/react-utility-hooks"
  },
  "homepage": "https://github.com/dalgudot/react-utility-hooks#readme",
  "bugs": {
    "url": "https://github.com/dalgudot/react-utility-hooks/issues"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test --watch",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "keywords": [
    "react",
    "hooks"
  ],
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "module": "dist/utility-hooks.esm.js",
  "size-limit": [
    {
      "path": "dist/utility-hooks.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/utility-hooks.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^7.0.5",
    "husky": "^7.0.4",
    "size-limit": "^7.0.5",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1"
  }
}
